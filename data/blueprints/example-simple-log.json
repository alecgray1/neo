{
  "id": "simple-log",
  "name": "Simple Log Blueprint",
  "version": "1.0.0",
  "description": "Logs a message when triggered",
  "variables": {},
  "nodes": [
    {
      "id": "event",
      "type": "neo/OnEvent",
      "position": { "x": 0, "y": 0 },
      "config": {
        "event_type": "test"
      }
    },
    {
      "id": "log",
      "type": "neo/Log",
      "position": { "x": 200, "y": 0 },
      "config": {
        "defaults": {
          "message": "Blueprint executed successfully!"
        }
      }
    }
  ],
  "connections": [
    { "from": "event.exec", "to": "log.exec" }
  ]
}
