{
  "id": "async-sequence",
  "name": "Async Sequence Blueprint",
  "version": "1.0.0",
  "description": "Demonstrates async execution with delays between log messages",
  "variables": {},
  "nodes": [
    {
      "id": "event",
      "type": "neo/OnEvent",
      "position": { "x": 0, "y": 0 },
      "config": {
        "event_type": "start_sequence"
      }
    },
    {
      "id": "log_start",
      "type": "neo/Log",
      "position": { "x": 200, "y": 0 },
      "config": {
        "defaults": {
          "message": "Starting async sequence..."
        }
      }
    },
    {
      "id": "delay1",
      "type": "neo/Delay",
      "position": { "x": 400, "y": 0 },
      "config": {
        "defaults": {
          "duration_ms": 2000
        }
      }
    },
    {
      "id": "log_step1",
      "type": "neo/Log",
      "position": { "x": 600, "y": 0 },
      "config": {
        "defaults": {
          "message": "Step 1 complete (after 2 second delay)"
        }
      }
    },
    {
      "id": "delay2",
      "type": "neo/Delay",
      "position": { "x": 800, "y": 0 },
      "config": {
        "defaults": {
          "duration_ms": 1000
        }
      }
    },
    {
      "id": "log_done",
      "type": "neo/Log",
      "position": { "x": 1000, "y": 0 },
      "config": {
        "defaults": {
          "message": "Async sequence complete!"
        }
      }
    }
  ],
  "connections": [
    { "from": "event.exec", "to": "log_start.exec" },
    { "from": "log_start.exec", "to": "delay1.exec" },
    { "from": "delay1.completed", "to": "log_step1.exec" },
    { "from": "log_step1.exec", "to": "delay2.exec" },
    { "from": "delay2.completed", "to": "log_done.exec" }
  ]
}
