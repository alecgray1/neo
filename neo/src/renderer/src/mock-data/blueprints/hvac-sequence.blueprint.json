{
  "id": "bp-hvac-seq-001",
  "name": "HVAC Startup Sequence",
  "version": "1.0.0",
  "description": "Sequential startup of HVAC equipment with delay between stages",
  "variables": {
    "startupDelay": {
      "type": "Real",
      "default_value": 30.0,
      "description": "Delay in seconds between stages"
    }
  },
  "nodes": [
    {
      "id": "node-1",
      "type": "neo/OnEvent",
      "position": { "x": 50, "y": 200 },
      "config": {
        "event": "occupancy_start"
      }
    },
    {
      "id": "node-2",
      "type": "neo/Sequence",
      "position": { "x": 250, "y": 200 },
      "config": {}
    },
    {
      "id": "node-3",
      "type": "neo/Log",
      "position": { "x": 500, "y": 100 },
      "config": {
        "level": "info"
      }
    },
    {
      "id": "node-4",
      "type": "neo/Delay",
      "position": { "x": 500, "y": 250 },
      "config": {}
    },
    {
      "id": "node-5",
      "type": "neo/GetVariable",
      "position": { "x": 300, "y": 350 },
      "config": {
        "variable": "startupDelay"
      }
    },
    {
      "id": "node-6",
      "type": "neo/Log",
      "position": { "x": 750, "y": 250 },
      "config": {
        "level": "info"
      }
    },
    {
      "id": "node-7",
      "type": "neo/Delay",
      "position": { "x": 500, "y": 400 },
      "config": {}
    },
    {
      "id": "node-8",
      "type": "neo/Log",
      "position": { "x": 750, "y": 400 },
      "config": {
        "level": "info"
      }
    }
  ],
  "connections": [
    {
      "from": "node-1.exec",
      "to": "node-2.exec"
    },
    {
      "from": "node-2.then_0",
      "to": "node-3.exec"
    },
    {
      "from": "node-2.then_1",
      "to": "node-4.exec"
    },
    {
      "from": "node-5.value",
      "to": "node-4.duration"
    },
    {
      "from": "node-4.completed",
      "to": "node-6.exec"
    },
    {
      "from": "node-2.then_2",
      "to": "node-7.exec"
    },
    {
      "from": "node-5.value",
      "to": "node-7.duration"
    },
    {
      "from": "node-7.completed",
      "to": "node-8.exec"
    }
  ]
}
